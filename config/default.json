{
  "host": "irc.enterservernamehere.org",
  "botnick": "botnamehere",
  "options": {
    "userName": "namegoeshere",
    "realName": "namegoeshere",
    "port": 6667,
    "localAddress": null,
    "debug": false,
    "showErrors": false,
    "autoRejoin": false,
    "autoConnect": true,
    "channels": ["#bots"]
  },
  "command-prefix": ".",
  "enabled-modules": [
    "url",
    "uptime",
    "irc",
    "seen"
  ],
  "modules": {
    "seen": {
      "database": "databasegoeshere",
      "username": "namegoeshere",
      "password": "passwordgoeshere",
      "opts": {
        "host": "localhost",
        "dbtype": "mysql",
        "logging": false
      }
    },
    "quote": {
      "database": "databasegoeshere",
      "username": "namegoeshere",
      "password": "passwordgoeshere",
      "opts": {
        "host": "localhost",
        "dbtype": "mysql",
        "logging": false
      }
    },
    "reddit-link-log": {
      "username": "namegoeshere",
      "password": "passwordgoeshere",
      "subreddit": "subredditgoeshere"
    }
  },
  "log4js": {
    "appenders": [
      {
        "type": "console"
      }, {
        "type": "dateFile",
        "filename": "logs/jarvis",
        "pattern": "-yyyyMMdd.log",
        "alwaysIncludePattern": true
      }
    ],
    "replaceConsole": true
  }
}
